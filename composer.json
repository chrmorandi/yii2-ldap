{
    "name": "chrmorandi/yii2-ldap",
    "description": "Ldap",
    "type": "library",
    "keywords": ["yii2", "extension", "ldap", "Active Directory", "OpenLDAP"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Mota",
            "email": "chrmorandi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "*",
        "ext-ldap": "*"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "yiisoft/yii2-coding-standards": "*"
    },
    "extra": {
        "asset-vcs-driver-options": {
            "github-no-api": true
        },
        "asset-pattern-skip-version": "(-build|-patch)"
    },
    "autoload": {
        "psr-4": {
            "chrmorandi\\ldap\\": "src"
        }
    }
}
